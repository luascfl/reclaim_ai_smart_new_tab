{
  "manifest_version": 3,
  "name": "Reclaim.ai Smart New Tab",
  "short_name": "Reclaim New Tab",
  "version": "1.3.2",
  "description": "Abre Reclaim.ai em uma nova aba somente se ainda n\u00e3o estiver aberto.",
  "default_locale": "pt_BR",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://app.reclaim.ai/*"
  ]
}
