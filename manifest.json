{
  "manifest_version": 2,
  "name": "Reclaim.ai Smart New Tab",
  "short_name": "Reclaim New Tab",
  "version": "1.3.1",
  "description": "Abre Reclaim.ai em uma nova aba somente se ainda n√£o estiver aberto.",
  "default_locale": "pt_BR",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "*://app.reclaim.ai/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "reclaim-handler@lucas.local",
      "data_collection_permissions": {
        "required": [
          "none"
        ],
        "optional": [],
        "has_previous_consent": false
      },
      "strict_min_version": "142.0"
    }
  }
}